# a generic filing cabinet



  Separator {  # begin cabinet

    Transform {

      translation -3.0 -1.2545 -3.5

      rotation 0.0 1.0 0.0 0.79

    }

    Separator {    # begin frame

      WWWAnchor {

        name ""

        description "Try a drawer..."

        Separator {                                # right side wall

          Translation { translation 0.25 0.0 0.0 }

          Material { diffuseColor 1.0 1.0 0.0 }

          DEF SideWall Cube {

              width 0.025

              height 1.0

              depth  0.75

          }

        }

        Separator {                                # left side wall

          Translation { translation -0.25 0.0 0.0 }

          Material { diffuseColor 1.0 1.0 0.0 }

          USE SideWall

        }

        Separator {                                 # top

          Translation { translation 0.0 0.5 0.0 }

          Material { diffuseColor 1.0 1.0 0.0 }

          DEF TopBottom Cube {

              width 0.525

              height 0.025

              depth  0.75

          }

        }

        Separator {                                 # bottom

          Material { diffuseColor 1.0 1.0 0.0 }

          Translation { translation 0.0 -0.5 0.0 }

          USE TopBottom

        }

        Separator {                                 # back

          Translation { translation 0.0 0.0 -0.375 }

          Material { diffuseColor 1.0 0.0 0.0 }

          DEF Back Cube {

            width 0.5

            height 1.0

            depth 0.025

          }

        }

      }

    }    # end frame



    Separator { # begin drawer

      WWWAnchor {

        name "http://www.vrml.com/models/test/cube.wrl"

        description "Bottom drawer contains a chain of test .wrl files"

        Transform {

          translation 0.0 0.0 0.25     # slide the drawer out

        }

        DEF aDrawer Separator {

          Material { diffuseColor 0.4 0.25 0.5 }

          Transform {

            translation 0.0 0.0 0.0

            scaleFactor 0.89 1.0 0.89

          }

          Separator {

            Transform {

              translation 0.0 -0.455 0.0

              scaleFactor 0.9 .9 0.9

            }

            USE TopBottom

          }

          Separator {

            Transform {

              translation -0.22 -0.25 0.0

              scaleFactor 1.0 0.445 0.89

            }

            USE SideWall

          }

          Separator {

            Transform {

              translation 0.22 -0.25 0.0

              scaleFactor 1.0 0.445 0.89

            }

            USE SideWall

          }

          Separator {

            Transform {

              translation 0.0 -0.25 -0.32

              scaleFactor 0.89 0.445 0.89

            }

            USE Back

          }

          Separator {

            Transform {

              translation 0.0 -0.25 0.32

              scaleFactor 0.89 0.445 0.89

            }

            USE Back

          }

        }

      }

    } # end drawer



    Separator { # begin TOP drawer

      WWWAnchor { 

        name ""

        description "Top drawer is empty for now, but may become LOD demo..."

        Separator {

          Transform { translation 0.0 0.5 0.125 }

          USE aDrawer

        }

      }

    }  # end 2nd drawer

  }  # end filing cabinet



# yer basic "Temple of Knowledge (Mark I)"



  Material { diffuseColor 0.91 0.76 0.65 }

  DEF aTemple Separator {

    Separator {

      Translation { translation 4.0 0.0 4.0 }

      DEF aColumn Separator {

        Separator {

          Translation { translation 0.0 1.6 0.0 }

          DEF pad Cube {

            width 1.0

            height 0.2

            depth 1.0

          }

        }

        Separator {                               #     the cylinder

          Cylinder {

            parts ALL

            radius 0.5

            height 3.0

          }

        }

        Separator {                               #     the second cube

          Translation { translation 0.0 -1.6 0.0 }

          USE pad

        }

      }

    }

    Separator {

      Translation { translation -4.0 0.0 4.0 }

      USE aColumn

    }

    Separator {

      Translation { translation 4.0 0.0 -4.0 }

      USE aColumn

    }

    Separator {

      Translation { translation -4.0 0.0 -4.0 }

      USE aColumn

    }

    Separator {

      Translation { translation 1.6 0.0 -4.0 }

      USE aColumn

    }

    Separator {

      Translation { translation -1.6 0.0 -4.0 }

      USE aColumn

    }

    Separator {

      Translation { translation 4.0 0.0 0.0 }

      USE aColumn

    }

    Separator {

      Translation { translation -4.0 0.0 0.0 }

      USE aColumn

    }

    Separator {                                   # and we can not forget the roof

      Material { diffuseColor 0.22 0.69 0.87 }

      Translation { translation 0.0 1.8 0.0 }

      Cube {

          width 9.0

          height 0.25

          depth 9.0

      }

    }

  }



